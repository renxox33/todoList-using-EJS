<%-include('header')%>
    
    <div class="box">
        <% newListItem.forEach((element) => { %>
            <div class="item">
                <input type="checkbox">
                <p> <%=element%> </p>
            </div>
            <% }) %>   
                
        <form class="item" method="POST" action="/">
            <input type="text" name='newTodoItem' placeholder="Add an item" autocomplete="off">
            <button type="submit">+</button>
        </form>    
        
    </div> 
</body>
</html>